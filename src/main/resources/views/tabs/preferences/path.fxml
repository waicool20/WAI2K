<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0"
      xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label text="SikuliX JAR path:"/>
        <Hyperlink fx:id="sikulixJarPathLink"/>
        <Label text="ADB executable path:"/>
        <Hyperlink fx:id="adbPathLink"/>
        <Label text="Assets directory path:"/>
        <Hyperlink fx:id="assetsDirPathLink"/>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
